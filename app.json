{
  "expo": {
    "name": "DarRide",
    "slug": "DarRide",
    "version": "1.0.0",
    "color": "#black",
    "orientation": "portrait",
    "icon": "./assets/images/iconns.png",
    "scheme": "casataxi",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/iconns.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFD54F"
    },
    "assetsBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.casataxi.app"
    },
    "config": {
      "googleMapsApiKey": "YOUR_GOOGLE_MAPS_API_KEY"
    }
  },
  "android": {
    "adaptiveIcon": {
      "backgroundColor": "#E6F4FE",
      "foregroundImage": "./assets/images/android-icon-foreground.png"
    },
    "config": {
      "googleMaps": {
        "apiKey": "YOUR_GOOGLE_MAPS_API_KEY"
      }
    },
    "edgeToEdgeEnabled": true,
    "predictiveBackGestureEnabled": false
  },
  "web": {
    "output": "static",
    "favicon": "./assets/images/favicon.png"
  },
  "plugins": [
    "expo-router",
    [
      {
        "image": "./assets/images/Redtaxi.png",
        "imageWidth": 200,
        "resizeMode": "contain",
        "backgroundColor": "#FFD54F",
        "dark": {
          "backgroundColor": "#FFD54F"
        },
        "assetsBundlePatterns": [
          "**/*"
        ]
      }
    ]
  ],
  "experiments": {
    "typedRoutes": true
  }
}
